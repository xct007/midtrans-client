{
	"name": "@xct007/midtrans-client",
	"version": "1.1.0",
	"description": "Unofficial Midtrans Payment API Client Library",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"engines": {
		"node": ">=18"
	},
	"scripts": {
		"build": "sh ./scripts/build",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"test": "sh ./scripts/test",
		"typecheck": "tsc --noEmit",
		"prepare": "npm run build",
		"prepublishOnly": "exit 0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xct007/midtrans-client.git"
	},
	"files": [
		"dist"
	],
	"directories": {
		"example": "examples"
	},
	"keywords": [
		"api",
		"midtrans",
		"payments",
		"client"
	],
	"author": "FrierenDv",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/xct007/midtrans-client/issues"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"homepage": "https://github.com/xct007/midtrans-client#readme",
	"devDependencies": {
		"@babel/preset-typescript": "^7.25.9",
		"@microsoft/api-extractor": "^7.55.1",
		"@swc/jest": "^0.2.36",
		"@trivago/prettier-plugin-sort-imports": "^4.3.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.7.9",
		"dotenv": "^16.4.5",
		"esbuild-plugin-file-path-extensions": "^2.1.4",
		"eslint": "^9.13.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"globals": "^15.11.0",
		"jest": "^29.7.0",
		"prettier": "^3.3.3",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsup": "^8.5.1",
		"typescript": "^5.6.3",
		"typescript-eslint": "^8.11.0"
	},
	"dependencies": {
		"undici": "^6.22.0"
	}
}
